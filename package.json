{
  "name": "storybook-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "pnpm storybook:build && npm-run-all -p -r storybook:start storybook:test",
    "storybook": "rimraf node_modules/.cache && storybook dev -p 9001",
    "storybook:build": "rimraf node_modules/.cache && storybook build",
    "storybook:start": "http-server -s -p 9001 storybook-static",
    "storybook:test": "test-storybook --url http://127.0.0.1:9001 --coverage",
    "lint:fix": "eslint --fix ./stories"
  },
  "dependencies": {
    "next": "16.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sass": "1.97.0",
    "styled-jsx": "5.1.7"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.3",
    "@eslint/js": "^9.39.2",
    "@storybook/addon-a11y": "10.1.10",
    "@storybook/addon-coverage": "3.0.0",
    "@storybook/addon-docs": "10.1.10",
    "@storybook/addon-onboarding": "10.1.10",
    "@storybook/addon-vitest": "10.1.10",
    "@storybook/nextjs-vite": "10.1.10",
    "@storybook/test-runner": "0.24.2",
    "@types/node": "25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-storybook": "10.1.10",
    "globals": "^16.5.0",
    "http-server": "14.1.1",
    "npm-run-all": "4.1.5",
    "playwright": "1.57.0",
    "rimraf": "6.1.2",
    "storybook": "10.1.10",
    "storybook-addon-vite-mock": "1.1.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0"
  },
  "main": "index.js",
  "repository": "https://github.com/SoraKumo001/storybook-addon-vite-mock-test",
  "author": "SoraKumo <info@croud.jp>",
  "license": "MIT"
}
